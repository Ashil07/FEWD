# Copy this to a file named .env and fill in the values
# Make sure REDIRECT_URI exactly matches the value registered in your Spotify Developer Dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
# Example (if running locally on port 8888):
REDIRECT_URI=http://localhost:8888/callback
# Optional: change the server port
PORT=8888
